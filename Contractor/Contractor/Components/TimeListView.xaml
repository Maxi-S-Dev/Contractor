<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:compontents="clr-namespace:Contractor.Components"
             x:Class="Contractor.Components.TimeListView">

    <Grid HorizontalOptions="Start" ColumnDefinitions="auto, auto" ColumnSpacing="10">
        <compontents:RoundProgressBar BindingContext="{Binding FreeTimeTimer}"/>
        <compontents:RoundProgressBar BindingContext="{Binding ProductiveTimer}" Grid.Column="1"/>
    </Grid>
    
</ContentView>
